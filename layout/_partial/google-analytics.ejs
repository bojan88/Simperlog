<% if (config.google_analytics){ %>
<!-- Google Analytics -->
<script type="text/javascript">
function initAnalytics() {
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

  ga('create', '<%= config.google_analytics %>', 'auto');
  ga('send', 'pageview');
}
if (window.addEventListener) window.addEventListener("DOMContentLoaded", initAnalytics, false);
else if (window.attachEvent) window.attachEvent("onload", initAnalytics);
else window.onload = initAnalytics;
</script>
<!-- End Google Analytics -->
<% } %>
